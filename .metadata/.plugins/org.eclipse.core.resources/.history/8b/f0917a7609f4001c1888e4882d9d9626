package entities;

public class EmployeeExercicioProposto {
	
	private Integer idCadaster;
	private Integer id;
	private String name;
	private Double salary;
	
	
	public EmployeeExercicioProposto(Integer idCadaster, Integer id, String name, Double salary) {
		this.idCadaster = idCadaster;
		this.id = id;
		this.name = name;
		this.salary = salary;
	}


	public Integer getIdCadaster() {
		return idCadaster;
	}


	public Integer getId() {
		return id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public Double getSalary() {
		return salary;
	}


	public void increaseSalary(double percentage) {
		salary += salary * (percentage / 100);
	}

}
